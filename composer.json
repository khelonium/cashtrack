{
    "name": "refactoring/cashtrack",
    "description": "Very Personal Finance",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://refactoring.ro",

    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">2.2.0rc1",
        "refactoring/time": "0.2.0",
        "zf-commons/zfc-user": "dev-master",
        "chrisboulton/php-resque": "1.2.x",
        "colinmollenhour/credis": "~1.2"


    },
    "require-dev": {
        "behat/behat": "3.*@stable",
        "behat/mink": "1.6.*@stable",
        "behat/mink-extension": "@stable",
        "behat/mink-goutte-driver": "@stable",
        "behat/mink-selenium2-driver": "@stable",
        "phpunit/phpunit": "*@stable"
    },

    "autoload": {
        "psr-4": {
            "Database\\": "apps/core/Database/src",
            "Reporter\\": "apps/core/Reporter/src",
            "Finance\\":  "apps/core/Finance/src",
            "Import\\": "apps/core/Import/src",
            "Prediction\\": "apps/core/Prediction",
            "Library\\": "apps/core/Library",
            "Watch\\": "apps/core/Watch",
            "Jobs\\": "apps/jobs"
        },
        "psr-0": {
        }
    }


}
